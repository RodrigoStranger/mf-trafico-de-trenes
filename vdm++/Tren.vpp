class Tren
  instance variables
    private idTrain: nat1;                
    private typeTrain: seq of char;    
    private distanceSafetyTrain: real;

  operations
    public Tren: nat1 * seq of char * real ==> Tren
    Tren(identificadorTren, tipoTren, distanciaSeguridadTren) ==                            
     (idTrain := identificadorTren;
      typeTrain := tipoTren;
      distanceSafetyTrain := distanciaSeguridadTren);

end Tren